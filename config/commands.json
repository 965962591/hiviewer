            "adb shell settings put global systemui_float_whitelist -196939687",
            "",
            "adb shell settings put global miui_permission_check 3",
            "",
            "adb shell settings put global miui_account_login_check 0",
            "",
            "adb shell settings put global device_provisioned 1",
            "",
            "adb shell am startservice -n com.miui.sysopt/.service.OmniService",
